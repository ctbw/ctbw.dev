    "notion-types": "^4.8.2",
    "notion-utils": "^4.8.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-notion-x": "^4.9.9",
    "react-scripts": "3.4.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.1"
